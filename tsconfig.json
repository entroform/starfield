{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "outDir": "dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "resolveJsonModule": false,
    "sourceMap": true,
    "strict": true,
    "target": "es6"
  }
}